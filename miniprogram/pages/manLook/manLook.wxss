.container_box{
    width: 100%;
    padding: 46rpx 46rpx ;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.model_Info{
    display: flex;
    width: 100%;
    /* background-color: red; */
    justify-content: space-between;

}
.model_item{
    border-radius: 32rpx 32rpx 32rpx 32rpx;
    width: 312rpx;
    height: 216rpx;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-size: 28rpx;
}
.model_people{
 
    background: #015CAD;
    
}
.model_car{
 
    background: #F78661;
    
}
.model_icon{
    width: 96rpx;
    height: 96rpx;
}

.statistic_box{
    margin-top: 32rpx;
    width: 654rpx;
height: 216rpx;
background: #F4F4F4;
border-radius: 32rpx 32rpx 32rpx 32rpx;
display: flex;
align-items: center;
justify-content: center;
color: #015CAD;
font-size: 28rpx;
}